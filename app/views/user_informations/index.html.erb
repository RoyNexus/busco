<p id="notice"><%= notice %></p>

<h1>Listing User Informations</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Family members</th>
      <th>Family description</th>
      <th>Pets</th>
      <th>Pets description</th>
      <th>Family workers</th>
      <th>Workers description</th>
      <th>Principal phone</th>
      <th>Secondary phone</th>
      <th>Secondary email</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_informations.each do |user_information| %>
      <tr>
        <td><%= user_information.name %></td>
        <td><%= user_information.family_members %></td>
        <td><%= user_information.family_description %></td>
        <td><%= user_information.pets %></td>
        <td><%= user_information.pets_description %></td>
        <td><%= user_information.family_workers %></td>
        <td><%= user_information.workers_description %></td>
        <td><%= user_information.principal_phone %></td>
        <td><%= user_information.secondary_phone %></td>
        <td><%= user_information.secondary_email %></td>
        <td><%= user_information.user_id %></td>
        <td><%= link_to 'Show', user_information %></td>
        <td><%= link_to 'Edit', edit_user_information_path(user_information) %></td>
        <td><%= link_to 'Destroy', user_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User information', new_user_information_path %>
